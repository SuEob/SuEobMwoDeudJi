<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.sueobmwodeudji.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_time_table"
        android:name="com.example.sueobmwodeudji.ui.timeTable.TimeTableFragment"
        android:label="@string/title_time_table"
        tools:layout="@layout/fragment_timetable" />

    <fragment
        android:id="@+id/navigation_community"
        android:name="com.example.sueobmwodeudji.ui.community.CommunityFragment"
        android:label="@string/title_community"
        tools:layout="@layout/fragment_community" />

    <fragment
        android:id="@+id/navigation_rating"
        android:name="com.example.sueobmwodeudji.ui.rating.RatingFragment"
        android:label="@string/title_rating"
        tools:layout="@layout/fragment_rating" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.sueobmwodeudji.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_setting" />
</navigation>